{
  "name": "Ti-build",
  "version": "1.0.1",
  "description": "App to run your Titanium projets",
  "main": "main/main.js",
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "start": "npm install && electron .",
    "pack": "build --dir",
    "dist": "build",
    "dev": "electron . --enable-logging"
  },
  "repository": "",
  "keywords": [
    "Electron"
  ],
  "author": "Nono",
  "license": "MIT",
  "dependencies": {
    "fix-path": "^2.1.0",
    "underscore": "latest",
    "xml2js": "latest"
  },
  "devDependencies": {
    "electron": "^1.6.11",
    "electron-builder": "^19.16.1"
  },
  "build": {
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
